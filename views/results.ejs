<h1>Results Page</h1>
<a href="/">Search Again</a>

    <% data["Search"].forEach(function(movie){ %>
    <div>
        <img src= <%= movie["Poster"] %> width="100px" height=auto>
        <strong>
            <%= movie["Title"] %>
        </strong>
        -
        <%= movie["Year"] %>
        <form id ="params" action="/details/<%= movie['imdbID'] %>" method="GET">
        <button type="submit">Details</form>
    </div>
    <% }) %>
    